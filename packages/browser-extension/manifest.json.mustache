{
  "name": "{{{ name }}}",
  "short_name": "{{{ name }}}",
  "version": "{{{ version }}}",
  "version_name": "{{{ versionName }}}"
  "manifest_version": 2,
  "author": "Conflux Network",
  "homepage_url": "https://github.com/Conflux-Chain/helios#readme",
  "description": "Fluent Crypto Wallet",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+Shift+F",
        "mac": "Alt+Shift+F",
        "chromeos": "Alt+Shift+F",
        "linux": "Alt+Shift+F"
      },
      "description": "Open the extension popup"
    },
    "inner_debug_only": {
      "suggested_key": {
        "mac": "Ctrl+MacCtrl+3"
      },
      "description": ""
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "19": "images/icon-19.png",
    "32": "images/icon-32.png",
    "38": "images/icon-38.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png",
    "512": "images/icon-512.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "fluent-wallet@confluxnetwork.org",
      "strict_min_version": "69.0"
    },
    "safari": {
      "strict_min_version": "14"
    }
  },
  "default_locale": "en",
  "background": {
    "scripts": [{{{backgroundScripts}}}],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "19": "images/icon-19.png",
      "32": "images/icon-32.png",
      "38": "images/icon-38.png",
      "64": "images/icon-64.png",
      "128": "images/icon-128.png",
      "512": "images/icon-512.png"
    },
    "default_title": "Fluent",
    "default_popup": "{{{popupHTML}}}"
  },
  "content_scripts": [
    {
      "matches": ["file://*/*", "http://*/*", "https://*/*"],
      "js": [{{{contentScripts}}}],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  {{{contentSecurityPolicy}}}
  "permissions": [
    "storage",
    "unlimitedStorage",
    "clipboardWrite",
    "http://localhost:12537/",
    {{{permissions}}}
    "https://confluxscan.net/*",
    "https://*.confluxscan.net/*",
    "activeTab",
    "notifications"
  ],
  "web_accessible_resources": [{{{webResources}}}],
  "externally_connectable": {
    "matches": [
      "https://confluxscan.net/*",
      "https://*.confluxscan.net/*",
      "https://confluxnetwork.org/*",
      "https://wallet-main.confluxrpc.org/*",
      "https://wallet-test.confluxrpc.org/*"
    ]
  }
}
