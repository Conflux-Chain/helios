{
  "name": "{{{ name }}}",
  "short_name": "{{{ name }}}",
  "version": "0.0.1",
  "manifest_version": 2,
  "author": "https://conflux-chain.org",
  "description": "Helios",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+Shift+H",
        "mac": "Alt+Shift+H",
        "chromeos": "Alt+Shift+H",
        "linux": "Alt+Shift+H"
      }
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "19": "images/icon-19.png",
    "32": "images/icon-32.png",
    "38": "images/icon-38.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png",
    "512": "images/icon-512.png"
  },
  "applications": {
    "gecko": {
      "id": "fluent@confluxnetwork.org",
      "strict_min_version": "56.0"
    }
  },
  "default_locale": "en",
  "background": {
    "scripts": [{{{backgroundScripts}}}],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "19": "images/icon-19.png",
      "32": "images/icon-32.png",
      "38": "images/icon-38.png",
      "64": "images/icon-64.png",
      "128": "images/icon-128.png",
      "512": "images/icon-512.png"
    },
    "default_title": "AHelios",
    "default_popup": "{{{popupHTML}}}"
  },
  "content_scripts": [
    {
      "matches": ["file://*/*", "http://*/*", "https://*/*"],
      "js": [{{{contentScripts}}}],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  {{{contentSecurityPolicy}}}
  "permissions": [
    "idle",
    "storage",
    "unlimitedStorage",
    "clipboardWrite",
    "http://localhost:12537/",
    {{{permissions}}}
    "https://confluxscan.io/*",
    "activeTab",
    "webRequest",
    "notifications"
  ],
  "web_accessible_resources": [{{{webResources}}}],
  "externally_connectable": {
    "matches": [
      "https://confluxscan.io/*",
      "https://testnet.confluxscan.io/*",
      "https://conflux-chain.org/*",
      "https://wallet-main.confluxrpc.org/*",
      "https://wallet-test.confluxrpc.org/*"
    ],
    "ids": ["*"]
  }
}
